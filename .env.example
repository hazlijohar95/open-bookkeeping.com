# Supabase
VITE_SUPABASE_URL="https://your-project.supabase.co"
VITE_SUPABASE_ANON_KEY="your-anon-key"
SUPABASE_SERVICE_KEY="your-service-key"

# Database (Supabase PostgreSQL)
DATABASE_URL="postgresql://postgres:[password]@db.[project-ref].supabase.co:5432/postgres"

# API
VITE_API_URL="http://localhost:3001"

# App
VITE_APP_URL="http://localhost:5173"

# MyInvois Gateway (Malaysia e-Invoice)
# Get credentials from https://sdk.myinvois.hasil.gov.my
MYINVOIS_GATEWAY_URL="http://localhost:3002"
MYINVOIS_GATEWAY_API_KEY="your-gateway-api-key"
MYINVOIS_ENVIRONMENT="SANDBOX"  # SANDBOX or PROD
MYINVOIS_CLIENT_ID="your-client-id"
MYINVOIS_CLIENT_SECRET="your-client-secret"

# Digital Certificate Paths (place files in ./certs/ directory)
# SIGNING_PRIVATE_KEY_PATH="./certs/private_key.pem"
# SIGNING_CERTIFICATE_PATH="./certs/certificate_base64.txt"

# Redis for Webhook Background Processing (Optional)
# BullMQ requires native Redis (not Upstash REST API)
# Without Redis, webhooks are dispatched synchronously
# BULLMQ_REDIS_URL="redis://localhost:6379"
# Or use individual settings:
# REDIS_HOST="localhost"
# REDIS_PORT="6379"
# REDIS_PASSWORD=""

# Error Monitoring (Optional)
# Get your DSN from https://sentry.io
# Backend (Node.js)
# SENTRY_DSN="https://xxxx@xxxx.ingest.sentry.io/xxxx"
# Frontend (React)
# VITE_SENTRY_DSN="https://xxxx@xxxx.ingest.sentry.io/xxxx"
