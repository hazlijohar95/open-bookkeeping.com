# Database (Supabase PostgreSQL)
DATABASE_URL="postgresql://postgres:[password]@db.[project-ref].supabase.co:5432/postgres"

# API
VITE_API_URL="http://localhost:3001"

# App
VITE_APP_URL="http://localhost:5173"

